{
  "Serilog": {
    "Using": [ "Serilog.Sinks.SQLite" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "SQLite",
        "Args": {
          "connectionString": "Data Source=D:\\SQLite\\WorkoutLog.db;",
          "tableName": "Logs",
          "autoCreateSqlTable": true
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "DatabaseSettings": {
    "ConnectionStrings": {
      "DefaultConnection": "Data Source=D:\\SQLite\\WorkoutLog.db;"
    },
    "ProviderName": "sqlite"
  }
}
