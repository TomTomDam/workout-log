{
  "Serilog": {
    "Using": [ "Serilog.Sinks.SQLite" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [],
    "Enrich": [ "FromLogContext" ]
  },
  "AllowedHosts": "*",
  "DatabaseSettings": {
    "ConnectionStrings": {
      "DefaultConnection": "Data Source=D:\\SQLite\\WorkoutLog.db;"
    },
    "ProviderName": "sqlite"
  }
}
